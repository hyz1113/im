{
  "name": "im",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "commit": "git-cz",
    "format": "prettier --write ./src",
    "clean:cache": "rm -rf node_modules && rm -rf .eslintcache && yarn install",
    "lint:eslint": "eslint --cache --fix --quiet --ext .vue,.js,.ts,.tsx src/",
    "lint-fix": "eslint --fix ./src --ext .vue,.js,.ts,.tsx",
    "lint:prettier": "prettier --write --loglevel warn \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{vue,css,scss,postcss,less}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:pretty": "pretty-quick --staged",
    "lint": "yarn lint:eslint && yarn lint:prettier  && yarn lint:pretty",
    "prepare": "husky install",
    "changelog": "standard-version"
  },
  "dependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.21.2",
    "axios": "^1.3.5",
    "core-js": "^3.8.3",
    "eslint": "^6.7.2",
    "event-source-polyfill": "^1.0.31",
    "fed-crowdin-cli": "^2.0.1",
    "js-cookie": "^3.0.1",
    "moment": "^2.29.4",
    "stylelint": "^14.2.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^32.0.0",
    "stylelint-order": "^6.0.3",
    "stylelint-scss": "^4.6.0",
    "tim-js-sdk": "^2.27.0",
    "tim-upload-plugin": "^1.1.0",
    "vant": "^4.2.0",
    "vconsole": "^3.15.0",
    "vue": "^3.2.13",
    "vue-class-component": "^8.0.0-0",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.0.3",
    "vuex": "^4.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.5.1",
    "@commitlint/config-conventional": "^17.4.4",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-plugin-typescript": "~5.0.0",
    "@vue/cli-plugin-vuex": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "babel-eslint": "^10.0.1",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^7.0.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^41.1.1",
    "eslint-plugin-vue": "^5.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "sass": "^1.48.0",
    "sass-loader": "8.0.2",
    "standard-version": "^9.5.0",
    "typescript": "~4.5.5"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended",
      "@vue/typescript/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "publishConfig": {
    "registry": "http://npm.huobiapps.com/"
  },
  "lint-staged": {
    "*.{js,vue,scss}": [
      "yarn lint",
      "git add"
    ]
  }
}
